pdf(file = "human_dros_roc.pdf", width = 8, height = 4)
layout(matrix(seq(2), nrow = 1))
par("mar" = c(4,4,4,4))
# Human ROC ####
plot(NULL, xlim = c(0,1), ylim = c(0,1), main = "Human Lungs",
xlab = "False positive rate", ylab = "True positive rate")
for(i in seq(ncol(a_tpm))){
myroc = get_Roc(log(a_tpm[,i]), log(i_tpm[,i]))
points(myroc[,1], myroc[,2], col = rgb(1,0.4,0,0.6), type = "s")
}
lung_probroc = get_Roc(a_probs$V2, i_probs$V2, range = seq(0,1,by=0.01))
points(lung_probroc[,1], lung_probroc[,2], type = "s", col = "blue")
points(sum(i_probs$V2 > 0.5)/length(i_probs$V2),
sum(a_probs$V2 > 0.5)/length(a_probs$V2), pch = 5)
points(sum(i_probs$V2 > 0.05)/length(i_probs$V2),
sum(a_probs$V2 > 0.05)/length(a_probs$V2), pch = 4)
points(sum(i_probs$V2 > 0.95)/length(i_probs$V2),
sum(a_probs$V2 > 0.95)/length(a_probs$V2), pch = 6)
points(c(0.6,0.6,0.6), c(0.4,0.3, 0.2), pch =c(4:6))
text(c(0.6,0.6,0.6), c(0.4,0.3, 0.2), labels = c(0.95, 0.5, 0.05), pos = 4)
abline(0,1,lty=2)
